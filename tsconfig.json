{
  "compilerOptions": {
    // "lib": ["esnext", "dom", "dom.iterable"],
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "moduleDetection": "force",
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "esModuleInterop": true
  }
}
